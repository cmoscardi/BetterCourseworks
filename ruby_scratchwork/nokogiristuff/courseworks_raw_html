	document.getElementById("showhide").innerHTML = (stl.display==''||stl.display=='block')?'Show past semester announcements':'<img src="mycrs/images/sidebardivider.gif" width="165" height="7" alt="" border="0"><br>Hide past semester announcements';
	stl.display = (stl.display==''||stl.display=='block')?'none':'block';
	}
	//-->
	</script>

	






